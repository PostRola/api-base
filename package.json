{
  "name": "bisa",
  "version": "0.0.1",
  "description": "Backend for Chidi",
  "main": "index.js",
  "scripts": {
    "check": "tsc",
    "build": "npm run check && webpack --env production",
    "dev": "concurrently npm:dev:server npm:dev:db",
