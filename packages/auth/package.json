{
  "name": "@webf/auth",
  "version": "0.4.0",
  "description": "Multi-tenant IAM system",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    "./context": "./dist/context.js",
    "./dal": "./dist/dal.js",
    "./db": "./dist/db/client.js",
    "./migrator": "./dist/db/migrator.js",
    "./result": "./dist/result.js",
    "./schema/*": "./dist/schema/*.js",
    "./util/*": "./dist/util/*.js"
  },
  "scripts": {
    "check:types": "tsc",
    "check:cycles": "tsx ./scripts/cycles.ts",
    "dev": "dotenv -- tsx ./src/main.ts",
    "prebuild": "rimraf ./dist",
    "build": "tsc -p ./tsconfig.build.json",
    "lint": "eslint ./src",
    "generate": "drizzle-kit generate:pg",
    "test": "NODE_OPTIONS='--import tsx' node --test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webf-run/identity.git"
  },
  "license": "SEE LICENSE IN ../../LICENSE",
  "keywords": [],
  "peerDependencies": {
    "postgres": "^3.4.3"
  },
  "dependencies": {
    "argon2": "^0.40.1",
    "crypto-random-string": "^5.0.0",
    "drizzle-orm": "^0.30.8",
    "nanoid": "^5.0.4",
    "oauth4webapi": "^2.10.4",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "drizzle-kit": "^0.20.14",
    "postgres": "^3.4.4"
  },
  "volta": {
    "node": "20.11.0"
  }
}
