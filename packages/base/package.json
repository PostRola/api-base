{
  "name": "@webf/base",
  "version": "0.6.0",
  "description": "Base system for building APIs",
  "type": "module",
  "exports": {
    ".": "./dist/web.js",
    "./package.json": "./package.json"
  },
  "scripts": {
    "check:types": "tsc",
    "check:cycles": "tsx ./scripts/cycles.ts",
    "dev": "dotenv -- tsx ./src/main.ts",
    "dev:pack": "tsx ./scripts/pack.ts",
    "prebuild": "rimraf ./dist",
    "build": "tsc -p ./tsconfig.build.json",
    "lint": "eslint ./src",
    "generate": "drizzle-kit generate:pg",
    "test": "NODE_OPTIONS='--import tsx' node --test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webf-run/identity.git"
  },
  "license": "SEE LICENSE IN ../../LICENSE",
  "keywords": [],
  "peerDependencies": {
    "@webf/auth": "*"
  },
  "dependencies": {
    "@hono/node-server": "^1.5.0",
    "hono": "^3.12.7",
    "oauth4webapi": "^2.8.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@webf/auth": "*"
  },
  "volta": {
    "node": "20.11.0"
  }
}
